<div>
  <div>
    <div>
      <h3>
        <%= @end_user.last_name %>
        <%= @end_user.first_name %>
        さんの会員情報
      </h3>
      <table>
        <tr>
          <th>会員ID</th>
          <td><%= @end_user.id %></td>
        </tr>
        <%= form_with model: @end_user, method: :patch do |f| %>
          <tr>
            <th>氏名</th>
            <td>
              <%= f.text_field :last_name %>
              <%= f.text_field :first_name %>
            </td>
          </tr>
          <tr>
            <th>フリガナ</th>
            <td>
              <%= f.text_field :last_name_kana %>
              <%= f.text_field :first_name_kana %>
            </td>
          </tr>
          <tr>
            <th>郵便番号</th>
            <td><%= f.text_field :postcode %></td>
          </tr>
          <tr>
            <th>住所</th>
            <td><%= f.text_field :address %></td>
          </tr>
          <tr>
            <th>電話番号</th>
            <td><%= f.text_field :phone_number %></td>
          </tr>
          <tr>
            <th>メールアドレス</th>
            <td><%= f.text_field :email %></td>
          </tr>
          <tr>
            <th>会員ステータス</th>
            <td>

            </td>
          </tr>
          <tr>
            <td><%= f.submit '変更を保存' %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
