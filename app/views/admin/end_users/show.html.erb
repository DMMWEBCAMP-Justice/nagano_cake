<div>
  <div>
    <div>
      <h2>
        <%= @end_user.last_name %>
        <%= @end_user.first_name %>
        さんの会員詳細
      </h2>
      <table>
        <tr>
          <th>会員ID</th>
          <td><%= @end_user.id %></td>
        </tr>
        <tr>
          <th>氏名</th>
          <td>
            <%= @end_user.last_name %>
            <%= @end_user.first_name %>
          </td>
        </tr>
        <tr>
          <th>フリガナ</th>
          <td>
            <%= @end_user.last_name_kana %>
            <%= @end_user.first_name_kana %>
          </td>
        </tr>
        <tr>
          <th>郵便番号</th>
          <td><%= @end_user.postcode %></td>
        </tr>
        <tr>
          <th>住所</th>
          <td><%= @end_user.address %></td>
        </tr>
        <tr>
          <th>電話番号</th>
          <td><%= @end_user.phone_number %></td>
        </tr>
        <tr>
          <th>メールアドレス</th>
          <td><%= @end_user.email %></td>
        </tr>
        <tr>
          <th>会員ステータス</th>
          <td></td>
        </tr>
      </table>
      <%= link_to '編集する', edit_admin_end_user_path(@end_user.id), class: "" %>
      <%= link_to '注文履歴一覧を見るあとで訂正！', admin_end_users_path %>
    </div>
  </div>
</div>
