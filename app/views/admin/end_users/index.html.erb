<div class='container'>
  <div class="row">
    <div class='col'>
      <h3>会員一覧</h3>
      <table class='table table-hover table-inverse'>
          <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
          <% @end_users.each do |end_user| %>
            <tr>
              <td><%= end_user.id %></td>
              <td>
                <%= link_to admin_end_user_path(end_user) do %>
                  <%= end_user.last_name %>
                  <%= end_user.first_name %>
                <% end %>
              </td>
              <td><%= end_user.email %></td>
              <td></td>
            </tr>
          <% end %>
      </table>
      <%= paginate @end_users %>
    </div>
  </div>
</div>